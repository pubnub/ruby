{
  ".pubnub.yml": [
    { "pattern": "^version: \"(.+)\"$", "cleared": true },
    { "pattern": "\\s+package-name: pubnub-((\\d+\\.?){2,}).gem$", "cleared": true },
    { "pattern": "/releases/download/(.*)/pubnub-.*.gem$", "cleared": false },
    { "pattern": "/releases/download/.*/pubnub-(.*).gem$", "cleared": true }
  ],
  "VERSION": [
    { "pattern": "(.+)", "cleared": true }
  ],
  "Gemfile.lock": [
    { "pattern": "^\\s{2,}pubnub \\((.+)\\)$", "cleared": true }
  ],
  "lib/pubnub/version.rb": [
    { "pattern": "^\\s{2,}VERSION = '(.+)'.freeze", "cleared": true }
  ]
}
